{
  "name": "react-analogue-time",
  "version": "0.1.0",
  "description": "A small and simple time picker for react based on google material UI",
  "main": "index.js",
  "scripts": {
    "build-dev": "node \"./node_modules/webpack/bin/webpack.js\" --config \"./webpack/dev.config.js\"",
    "build-prod": "node \"./node_modules/webpack/bin/webpack.js\" --config \"./webpack/production.config.js\"",
    "build": "npm run build-dev && npm run build-prod",
    "develop": "node \"./node_modules/webpack/bin/webpack.js\" --config \"./webpack/dev.config.js\" --watch",
    "release": "npm run build && node \"./tools/publish.js\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ShaneGH/react-analogue-time.git"
  },
  "keywords": [
    "ui",
    "material",
    "timepicker",
    "time",
    "react"
  ],
  "author": "Shane Connon",
  "license": "MIT",
  "dependencies": {
    "analogue-time-picker": "^1.0.2"
  },
  "devDependencies": {
    "@types/react": "^16.3.14",
    "@types/react-dom": "^16.0.5",
    "awesome-typescript-loader": "^5.0.0",
    "fs-walker": "^1.0.0",
    "ncp": "^2.0.0",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "typescript": "^2.8.3",
    "webpack": "^4.8.3",
    "webpack-cli": "^2.1.3",
    "webpack-merge": "^4.1.2"
  }
}
